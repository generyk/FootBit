<h1><%=@team.name%></h1>
<h2>By: <%=link_to @team.user.username, user_path(@team.user)%> </h2>

<div><%= @team.location%></div>

<p><%= link_to 'See all tryouts', team_tryout_path(@team)%></p>
<p><%= link_to 'Add new tryouts', new_team_tryout_path(@team)%></p>

<h4>Tryouts:</h4>
<% @team.tryouts.each do |c|%>
  <li><strong><%= c.user.username%></strong> says "<%= link_to c.location, tryout_path(c)%>"</li>
<% end %>
